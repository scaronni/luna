[Unit]
Description=Luna PED Server
After=network.target nss-lookup.target

[Service]
ExecStart=PedServer -configfile /etc/pedServer.conf -logfilename /var/log/luna/remotePedServerLog.log -m start
ExecStop=PedServer -configfile /etc/pedServer.conf -logfilename /var/log/luna/remotePedServerLog.log -m stop
Restart=on-failure
Type=forking

[Install]
WantedBy=multi-user.target
